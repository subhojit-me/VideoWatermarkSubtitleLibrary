<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup> 
      <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <add key="databaseIP" value="SUBHOJIT-databaseIP"/>
    <add key="initialDatabaseCatalog" value="databaseName"/>
    <add key="databaseUserID" value="sa"/>
    <add key="databasePassword" value="databasePassword"/>
    <add key="serviceRunDurationMinutes" value="1"/>
    <add key="watermarkPosition" value="top-right" /><!-- Options : "center","top-left","bottom-left","top-right","bottom-right" -->
    <add key="watermarkFileName" value="fact-logo.png" />
  </appSettings>
</configuration>